/dts-v1/;

/memreserve/ 0x00000000 0x00010000;

/ {
	model = "Cavium Thunder";
	compatible = "cavium,thunder";
	interrupt-parent = <&gic0>;
	#address-cells = <2>;
	#size-cells = <2>;

	aliases {
		serial0 = &uaa0;
		serial1 = &uaa1;
	};

	psci {
		compatible = "arm,psci";
		method = "smc";
		cpu_off = <0x84000002>;
		cpu_on = <0xC4000003>;
	};

