		#size-cells = <2>;
		#address-cells = <3>;
        	reg = <0x8490 0x00000000 0 0x40000000>;  /* Configuration space */
		ranges = <0x03000000 0x8310 0x00000000 0x8310 0x00000000 0x00 0x10000000>, /* mem ranges */
			<0x03000000 0x8100 0x00000000 0x8100 0x00000000 0x80 0x00000000>;
        };
	
